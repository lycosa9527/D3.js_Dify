# Example .env file for D3_Graph_Gen
# Flask App
HOST=0.0.0.0
PORT=9527
DEBUG=False
# Qwen API (Required - for core application functionality)
QWEN_API_KEY=your-qwen-api-key-here
QWEN_API_URL=https://dashscope.aliyuncs.com/compatible-mode/v1/chat/completions
QWEN_MODEL=qwen-plus
QWEN_TEMPERATURE=0.7
QWEN_MAX_TOKENS=1000
QWEN_TIMEOUT=40

# DeepSeek API (Optional - for development phase and optional production features)
# Application will run without these settings, using Qwen as default
DEEPSEEK_API_KEY=your-deepseek-api-key-here
DEEPSEEK_API_URL=https://api.deepseek.com/v1/chat/completions
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_TEMPERATURE=0.7
DEEPSEEK_MAX_TOKENS=2000
DEEPSEEK_TIMEOUT=60
# Graph Language
GRAPH_LANGUAGE=zh
# UI/Graph Styling (optional)
TOPIC_FONT_SIZE=18
CHAR_FONT_SIZE=14
# Watermark (optional)
WATERMARK_TEXT=MindSpring 